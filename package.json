{
  "name": "ReactNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest --detectOpenHandles --silent=false",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "watchman watch-del-all && rm -rf $TMPDIR/react-* && react-native start --reset-cache",
    "typescript": "tsc --noEmit",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext=.js,.jsx,.ts,.tsx --fix",
    "pods:install": "npx pod-install",
    "prepare": "husky",
    "pods:clean": "cd ios && pod deintegrate && pod cache clean --all && npx pod-install && cd ../",
    "asset": "npx react-native-asset",
    "xcode": "xed -b ios",
    "postinstall": "patch-package",
    "watchman": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && pnpm store prune && pnpm install",
    "android:uninstall": "cd android && ./gradlew uninstallAll",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:apk": "cd android && ./gradlew assembleRelease && cd ../",
    "android:bundle": "cd android && ./gradlew bundleRelease && cd ../"
  },
  "dependencies": {
    "@enkeapple/react-native-hooks": "1.0.3",
    "@gorhom/bottom-sheet": "^5.0.5",
    "@gorhom/portal": "1.0.14",
    "@react-navigation/elements": "1.3.31",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1",
    "@reduxjs/toolkit": "2.3.0",
    "@testing-library/react-native": "^12.8.1",
    "i18next": "21.10.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-junit": "^16.0.0",
    "patch-package": "8.0.0",
    "react": "18.3.1",
    "react-hook-form": "^7.53.1",
    "react-i18next": "11.18.6",
    "react-native": "0.75.4",
    "react-native-bootsplash": "6.2.6",
    "react-native-config": "1.5.3",
    "react-native-device-info": "14.0.0",
    "react-native-gesture-handler": "2.20.2",
    "react-native-inappbrowser-reborn": "3.7.0",
    "react-native-keyboard-controller": "1.14.3",
    "react-native-mmkv": "^2.12.2",
    "react-native-modal": "13.0.1",
    "react-native-reanimated": "3.15.5",
    "react-native-safe-area-context": "4.13.1",
    "react-native-screens": "3.35.0",
    "react-native-svg": "14.2.0",
    "react-native-svg-mock": "^2.0.0",
    "react-native-toast-message": "2.2.0",
    "react-native-unistyles": "^2.11.0",
    "react-native-vector-icons": "10.2.0",
    "react-redux": "^9.1.2",
    "react-test-renderer": "^18.3.1",
    "reactotron-react-native": "^5.1.9",
    "reactotron-react-native-mmkv": "^0.2.7",
    "reactotron-redux": "^3.1.10",
    "redux-logger": "^3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "3.1.0",
    "ts-jest": "^29.2.4",
    "ts-pattern": "5.2.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.4",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "^29.5.14",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.3.0",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-native": "4.1.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.0.3",
    "react-native-svg-transformer": "1.5.0",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
