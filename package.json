{
  "name": "ReactNativeTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "npx react-native start --reset-cache",
    "typescript": "tsc --noEmit",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "cocoapods": "cd ios && pod deintegrate && pod cache clean --all && pod install && cd ../",
    "asset": "npx react-native-asset",
    "xcode": "xed -b ios",
    "flipper": "npx flipper-server",
    "watchman": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules/ && npm cache verify && npm install --force",
    "android:uninstall": "cd android && ./gradlew uninstallAll",
    "android:clean": "cd android && ./gradlew clean && cd ../",
    "android:apk": "cd android && ./gradlew assembleRelease && cd ../",
    "android:bundle": "cd android && ./gradlew bundleRelease && cd ../",
    "codepush:dev:android": "appcenter codepush release-react -a application/name -d Development",
    "codepush:dev:ios": "appcenter codepush release-react -a application/name -d Development",
    "codepush:prod:android": "appcenter codepush release-react -a application/name -d Production",
    "codepush:prod:ios": "appcenter codepush release-react -a application/name -d Production"
  },
  "dependencies": {
    "@enkeapple/react-native-hooks": "^1.1.10",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@gorhom/portal": "1.0.14",
    "@react-navigation/elements": "1.3.30",
    "@react-navigation/native": "6.1.17",
    "@react-navigation/stack": "6.3.29",
    "@reduxjs/toolkit": "2.2.4",
    "i18next": "21.10.0",
    "patch-package": "8.0.0",
    "react": "18.3.1",
    "react-i18next": "11.18.4",
    "react-native": "0.74.1",
    "react-native-bootsplash": "5.5.3",
    "react-native-config": "1.5.1",
    "react-native-device-info": "10.13.2",
    "react-native-gesture-handler": "2.16.2",
    "react-native-inappbrowser-reborn": "3.7.0",
    "react-native-keyboard-controller": "1.12.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-modal": "13.0.1",
    "react-native-reanimated": "3.11.0",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "14.1.0",
    "react-native-toast-message": "2.2.0",
    "react-native-vector-icons": "10.1.0",
    "react-redux": "9.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "3.1.0",
    "styled-components": "6.1.11",
    "ts-pattern": "5.1.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.4",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/metro-config": "0.74.83",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.1.0",
    "husky": "9.0.11",
    "prettier": "3.0.3",
    "react-native-svg-transformer": "1.3.0",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
